<h2>Logged in as: <%= @user.username %></h2>

<a href="/users/<%= @user.id %>">View Account</a>

<a href="/users/<%= @user.id %>/edit">Update Account</a>

<h3>Renovator Users:</h3>

<% User.all.each do |user| %>
<ul>
    <li><strong><%= user.id %>. <%= user.username %></strong> | <%= user.location %></li>
</ul>
<% end %>